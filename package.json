    "repository": {
        "type": "git",
        "url": "git+https://github.com/mdconaway/sails-ember-rest.git"
    },
    "bugs": {
        "url": "https://github.com/mdconaway/sails-ember-rest/issues"
    },
    "homepage": "https://github.com/mdconaway/sails-ember-rest#readme",
    "author": "Michael Conaway",
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "dependencies": {
        "lodash": "^4.0.0",
        "merge-defaults": ">=0.1.0",
        "pluralize": "^1.1.0"
    },
    "devDependencies": {
        "async": "2.0.1",
        "babel": "^6.23.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-preset-es2015": "^6.24.1",
