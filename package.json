    "scripts": {
        "start": "node app.js",
        "test": "node ./node_modules/mocha/bin/mocha test/lifecycle.test.js test/integration/**/*.test.js",
        "format": "node ./node_modules/prettier/bin/prettier --single-quote --tab-width 4 --print-width 120 --trailing-comma none --write \"{api/**/*.js,config/**/*.js,test/**/*.js,templates/**/*.js,bin/*.js,index.js,app.js}\""
